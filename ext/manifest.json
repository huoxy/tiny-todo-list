{
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "version": "2.7",
  "manifest_version": 2,
  "default_locale": "en",
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "16": "build/icon_16.png",
      "24": "build/icon_24.png",
      "32": "build/icon_32.png"
    },
    "default_popup": "build/popup.html"
  },
  "icons": {
    "16": "build/icon_16.png",
    "24": "build/icon_24.png",
    "128": "build/icon_128.png"
  },
  "permissions": [
    "alarms",
    "storage",
    "background",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "persistent": false
  }
}
